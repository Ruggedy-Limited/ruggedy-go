{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.3.*",
        "laravel/cashier": "~7.0",
        "laravel/spark": "*@dev",
        "laravelcollective/annotations": "~5.3.0",
        "laravelcollective/html": "~5.3.0",
        "laravel-doctrine/orm": "~1.2.8",
        "laravel-doctrine/extensions": "~1.0.8",
        "beberlei/doctrineextensions": "~1.0",
        "gedmo/doctrine-extensions": "~2.4",
        "laravel-doctrine/migrations": "~1.1.0",
        "laracasts/flash": "~2.0.0",
        "mysql-workbench-schema-exporter/mysql-workbench-schema-exporter": "^3.0",
        "mysql-workbench-schema-exporter/doctrine2-exporter": "^3.0",
        "madewithlove/tactician-laravel": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6.0",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
        "behat/behat": "~3.2.0",
        "behat/mink": "~1.7.1",
        "behat/mink-extension": "~2.2",
        "laracasts/behat-laravel-extension": "~1.0",
        "behat-extension/doctrine-data-fixtures-extension": "~4.0.0",
        "barryvdh/laravel-ide-helper": "~2.2.1",
        "barryvdh/laravel-debugbar": "~2.3.0",
        "filp/whoops": "~2.1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./spark"
        }
    ]
}
